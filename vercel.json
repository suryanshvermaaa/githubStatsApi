{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "assets/**",
          "cards/**",
          "controllers/**",
          "github/**",
          "routes/**",
          "app.js"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/app.js" }
  ]
}
